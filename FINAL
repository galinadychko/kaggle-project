   "source": [
    "def read_prediction(path):\n",
    "    circles = []\n",
    "    df = pd.read_csv(path, names = [\"friend\", \"group\"], skiprows = 1)\n",
    "    ego = df[\"friend\"][0]\n",
    "    df = df.drop(df.index[0])\n",
    "    num_groups = df[\"group\"].max(axis = 0)\n",
    "    for i in range(1, num_groups + 1):\n",
    "        circles += [set(df[\"friend\"][df[\"group\"] == i])]\n",
    "    return ego, circles"
   ]
  },
